def problem_11(min = 90)
  big_prod = 0
  dirs = [[0,1],[1,0],[1,1],[0,-1],[-1,0],[-1,-1],[-1,1],[1,-1]]
  max_x = @data_11.count
  max_y = @data_11.first.count

  while min >= 50
    @data_11.each_with_index do |row,r|
      row.each_with_index do |val,c|
        dirs.each do |d|
          p = val
          for i in (1..3)
            x = (d.first * i) + r
            y = (d.last * i) + c
            if x < 0 ||
                y < 0 ||
                x >= max_x ||
                y >= max_y 
              next
            end
            p *= @data_11[x][y]
          end
          big_prod = p if p > big_prod
        end
      end
    end
    min -= 10
  end
  puts "problem_11 => #{big_prod}"
end
